{
  "$schema": "https://schemas.wp.org/wp/6.7/theme.json",
  "version": 3,
  "settings": {
    "color": {
      "defaultPalette": false,
      "defaultGradients": false,
      "defaultDuotone": false,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "duotone": [],
      "gradients": [],
      "link": false,
      "palette": [
        {
          "slug": "black",
          "color": "#000000",
          "name": "Black"
        },
        {
          "slug": "black-700",
          "color": "#050709",
          "name": "Black 700"
        },
        {
          "slug": "white",
          "color": "#FFFFFF",
          "name": "White"
        },
        {
          "slug": "purple-900",
          "color": "#0F0715",
          "name": "Purple 900"
        },
        {
          "slug": "purple-700",
          "color": "#140C1C",
          "name": "Purple 700"
        },
        {
          "slug": "purple-600",
          "color": "#2A1454",
          "name": "Purple 600"
        },
        {
          "slug": "purple-500",
          "color": "#8750F7",
          "name": "Purple 500"
        },
        {
          "slug": "purple-300",
          "color": "#C7A6FF",
          "name": "Purple 300"
        }
      ]
    },
    "custom": {},
    "layout": {
      "contentSize": "1280px",
      "wideSize": "1350px"
    },
    "spacing": {
      "blockGap": true,
      "margin": false,
      "padding": false,
      "spacingSizes": [
        {
          "size": "clamp(0.25rem, 0.0521rem + 0.5277vw, 0.5rem)",
          "slug": "fluid-8",
          "name": "8px (Fluid)"
        },
        {
          "size": "clamp(0.5rem, 0.1042rem + 1.0554vw, 1rem)",
          "slug": "fluid-16",
          "name": "16px (Fluid)"
        },
        {
          "size": "clamp(0.75rem, 0.1563rem + 1.5831vw, 1.5rem)",
          "slug": "fluid-24",
          "name": "24px (Fluid)"
        },
        {
          "size": "clamp(1rem, 0.2084rem + 2.1108vw, 2rem)",
          "slug": "fluid-32",
          "name": "32px (Fluid)"
        },
        {
          "size": "clamp(1.25rem, 0.2606rem + 2.6385vw, 2.5rem)",
          "slug": "fluid-40",
          "name": "40px (Fluid)"
        },
        {
          "size": "8px",
          "slug": "fixed-8",
          "name": "8px (Fixed)"
        },
        {
          "size": "12px",
          "slug": "fixed-12",
          "name": "12px (Fixed)"
        },
        {
          "size": "16px",
          "slug": "fixed-16",
          "name": "16px (Fixed)"
        },
        {
          "size": "36px",
          "slug": "fixed-36",
          "name": "36px (Fixed)"
        }
      ]
    },
    "typography": {
      "fluid": true,
      "customFontSize": false,
      "lineHeight": true,
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "\"Sora\", sans-serif",
          "name": "Primary, Sora",
          "slug": "primary",
          "fontFace": [
            {
              "fontFamily": "Sora",
              "fontStyle": "normal",
              "fontWeight": "300",
              "src": [
                "https://fonts.googleapis.com/css2?family=Sora:wght@300&display=swap"
              ]
            },
            {
              "fontFamily": "Sora",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": [
                "https://fonts.googleapis.com/css2?family=Sora:wght@400&display=swap"
              ]
            },
            {
              "fontFamily": "Sora",
              "fontStyle": "normal",
              "fontWeight": "500",
              "src": [
                "https://fonts.googleapis.com/css2?family=Sora:wght@500&display=swap"
              ]
            },
            {
              "fontFamily": "Sora",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": [
                "https://fonts.googleapis.com/css2?family=Sora:wght@700&display=swap"
              ]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "slug": "small",
          "size": "0.875rem",
          "fluid": {
            "min": "0.75rem",
            "max": "0.875rem"
          },
          "name": "Small"
        },
        {
          "slug": "normal",
          "size": "1rem",
          "fluid": {
            "min": "0.875rem",
            "max": "1rem"
          },
          "name": "Normal"
        },
        {
          "slug": "large",
          "size": "1.25rem",
          "fluid": {
            "min": "1rem",
            "max": "1.25rem"
          },
          "name": "Large"
        },
        {
          "slug": "x-large",
          "size": "1.5rem",
          "fluid": {
            "min": "1.25rem",
            "max": "1.5rem"
          },
          "name": "X-Large"
        },
        {
          "slug": "xx-large",
          "size": "1.625rem",
          "fluid": {
            "min": "1.5rem",
            "max": "1.625rem"
          },
          "name": "XX-Large"
        },
        {
          "slug": "xxx-large",
          "size": "2rem",
          "fluid": {
            "min": "1.625rem",
            "max": "2rem"
          },
          "name": "XXX-Large"
        },
        {
          "slug": "xxxx-large",
          "size": "2.5rem",
          "fluid": {
            "min": "2rem",
            "max": "2.5rem"
          },
          "name": "XXXX-Large"
        },
        {
          "slug": "display-md",
          "size": "4.5rem",
          "fluid": {
            "min": "3rem",
            "max": "4.5rem"
          },
          "name": "Display Medium"
        },
        {
          "slug": "display-lg",
          "size": "6.25rem",
          "fluid": {
            "min": "3.5rem",
            "max": "6.25rem"
          },
          "name": "Display Large"
        },
        {
          "slug": "fixed-12",
          "size": "12px",
          "name": "12px (Fixed)"
        },
        {
          "slug": "fixed-16",
          "size": "16px",
          "name": "16px (Fixed)"
        },
        {
          "slug": "fixed-24",
          "size": "24px",
          "name": "24px (Fixed)"
        }
      ]
    },
    "blocks": {
      "core/button": {
        "color": {
          "palette": []
        }
      }
    }
  },
  "styles": {
    "color": {},
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--display-md)",
          "fontWeight": "var(--font-weight-bold)",
          "lineHeight": "var(--font-line-height-sm)"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--xxxx-large)",
          "fontWeight": "var(--font-weight-bold)",
          "lineHeight": "var(--font-line-height-sm)"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--xxx-large)",
          "fontWeight": "var(--font-weight-bold)",
          "lineHeight": "var(--font-line-height-sm)"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "var(--font-weight-bold)",
          "lineHeight": "var(--font-line-height-sm)"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--normal)",
          "fontWeight": "var(--font-weight-bold)",
          "lineHeight": "var(--font-line-height-md)"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "var(--font-weight-bold)",
          "lineHeight": "var(--font-line-height-md)"
        }
      },
      "link": {},
      "button": {
        "border": {
          "radius": "50"
        },
        "color": {
          "background": "var(--wp--preset--color--purple-500)",
          "text": "var(--wp--preset--color--white)"
        },
        "spacing": {
          "padding": {
            "top": "13px",
            "right": "33px",
            "bottom": "13px",
            "left": "33px"
          }
        },
        "typography": {
          "fontWeight": "700",
          "fontSize": "var(--wp--preset--font-size--fixed-16)"
        },
        ":focus": {
          "outline": {
            "offset": "4px",
            "width": "2px",
            "style": "dashed",
            "color": "var(--wp--preset--color--purple-500)"
          }
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--primary)",
      "fontWeight": "var(--font-weight-normal)",
      "fontSize": "var(--wp--preset--font-size--normal)",
      "lineHeight": "var(--font-line-height-lg)"
    },
    "blocks": {}
  }
}
